{
  "name": "aiqx-use-cases",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host=0.0.0.0",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "build:production": "ng build --configuration production --aot --base-href ./",
    "build:staging": "ng build -c staging",
    "shell:build": "docker-compose -f docker-compose.shell.yml build --no-cache",
    "shell": "docker-compose -f docker-compose.shell.yml build && docker-compose -f docker-compose.shell.yml run --rm -u 1000 --service-ports --name aiqx-use-cases app bash || true && echo Stopping environment... && npm run shell:stop",
    "shell:join": "docker exec -it aiqx-use-cases bash || true",
    "shell:stop": "docker-compose -f docker-compose.shell.yml down",
    "test": "ISTANBUL_ADD_REPORTERS=text-summary ng test",
    "test:ci": "ISTANBUL_ADD_REPORTERS=text,json-summary KARMA_ADD_REPORTERS=junit ng test --watch=false --progress=false && npm run check",
    "check": "npm run check:lint && npm run check:format",
    "check:lint": "ng lint --eslint-config .eslintrc.json",
    "check:format": "prettier --check '{e2e/src,src}/**/*.{ts,js,html,css,scss,sass,less}'",
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:lint": "ng lint --fix --eslint-config .eslintrc.fix.json",
    "fix:format": "prettier --write '{e2e/src,src}/**/*.{ts,js,html,css,scss,sass,less}'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.8",
    "@angular/cdk": "^13.3.8",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "^13.3.8",
    "@angular/material": "^13.3.8",
    "@angular/material-moment-adapter": "^13.3.8",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@tinymce/tinymce-angular": "^6.0.1",
    "http-errors": "^1.8.1",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.20",
    "moment": "^2.29.3",
    "ng-debug-levels": "^0.2.3",
    "ng-gallery": "^6.0.1",
    "ng-table-virtual-scroll": "^1.4.0",
    "ng2-file-upload": "^1.4.0",
    "ngx-cookie-service": "^13.2.0",
    "ngx-file-drop": "^13.0.0",
    "ngx-loader-indicator": "^10.0.0",
    "ngx-mask": "^13.1.15",
    "rxjs": "^6.6.7",
    "tinymce": "^5.10.5",
    "tslib": "^2.4.0",
    "url-join": "^4.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/language-service": "^13.3.8",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@elunic/ecs-commitlint": "^0.1.3",
    "@elunic/ecs-prettier": "^0.2.2",
    "@elunic/ecs-tsconfig": "^0.1.1",
    "@elunic/eslint-config-ecs": "^8.0.0",
    "@ngrx/schematics": "^13.2.0",
    "@types/debug": "^4.1.7",
    "@types/http-errors": "^1.8.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/karma": "^6.3.3",
    "@types/karma-chrome-launcher": "^3.1.1",
    "@types/karma-coverage-istanbul-reporter": "^2.1.1",
    "@types/karma-jasmine": "0.0.31",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.20.51",
    "@types/prettier": "^2.6.0",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-sort-cli": "^2.0.14",
    "jsonwebtoken": "^8.5.1",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "^4.6.4"
  }
}
